# Cloudflare Configuration
# Copy this file to .env for deployment
# DO NOT commit .env to git!

# Cloudflare Account ID
# Found in Cloudflare dashboard: URL > /accounts/<account_id>
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Cloudflare API Token
# Create at: https://dash.cloudflare.com/profile/api-tokens
# Required permissions: Account > Cloudflare Pages > Edit
CLOUDFLARE_API_TOKEN=your_api_token_here

# Optional: Enable analytics in Pages Functions
ENABLE_ANALYTICS=false

# Optional: Custom domain for preview deployments
PREVIEW_DOMAIN=pages.dev
